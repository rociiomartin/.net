/*
12. Escribir un programa que imprima todos los divisores de un número entero ingresado 
desde la consola. Para obtener el entero desde un string st utilizar int.Parse(st).
*/
System.Console.WriteLine("Ingrese un numero");
int num=int.Parse(Console.ReadLine());
int i;
System.Console.WriteLine("Los divisores de "+num+":");
if(num >0)
{
    for (i = 1; i <= num; i++)
    {
        if (num % i==0)
        {
        Console.WriteLine(i);
        }
    }
}
else
   Console.WriteLine("El número debe ser mayor a 0");
